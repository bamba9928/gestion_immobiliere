{
  "name": "mada-immo",
  "version": "1.0.0",
  "description": "Plateforme MADA IMMO - Styles Tailwind",
  "scripts": {
    "dev": "npx tailwindcss -i ./static/css/input.css -o ./static/css/output.css --watch",
    "build": "npx tailwindcss -i ./static/css/input.css -o ./static/css/output.css --minify",
    "build:prod": "NODE_ENV=production npx tailwindcss -i ./static/css/input.css -o ./static/css/output.css --minify"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "tailwindcss": "^3.4.18"
  },
  "keywords": [
    "django",
    "tailwindcss",
    "mada-immo"
  ],
  "author": "MADA IMMO Team",
  "license": "MIT"
}